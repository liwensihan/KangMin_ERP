	</select>
	<select id="findDimRes" resultMap="BaseResultMap" parameterType="Map">
		SELECT res.* FROM erp_drug_result d ,erp_resuit res ,erp_kinds kin
		<where>
		d.RES_ID=res.RES_ID AND d.KIN_ID=kin.KIN_ID  and
    	<if test="pricer != ''">
    		(
    			res.RES_NAME like CONCAT('%',#{pricer},'%' )
    			or
	    		res.RES_SERIAL like CONCAT('%',#{pricer},'%' ) 
	    		or 
	    		res.REMARK like CONCAT('%',#{pricer},'%' )
	    		or 
	    		d.KIN_ID like CONCAT('%',#{pricer},'%' )
    		)
    	</if>
    	and res.ISVA =0;
    </where>
	</select>
